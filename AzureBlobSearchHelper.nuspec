<?xml version="1.0" encoding="utf-8"?>
<package>
  <metadata>
    <id>AzureBlobSearchHelper</id>
    <version>0.1.6-preview</version>
    <title>Helper class for storing meta data and files and running queries against them using azure blob store and search services</title>
    <authors>Anthony Baxter</authors>
    <owners>Anthony Baxter</owners>
    <requireLicenseAcceptance>false</requireLicenseAcceptance>
    <description>Helper for storing files and metadata in blob storage and searching an index created over them</description>
    <summary>It is referencable by all &gt;=.NET45 clients, including WindowsStore and Phone</summary>
    <dependencies>
      <group targetFramework="net">
	        <dependency id="System.Collections" version="4.0.0" />
			<dependency id="System.Diagnostics.Debug" version="4.0.0" />
